{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "expresbase",
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleDetection": "force",
    "noEmit": false,           
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2022",
  }
}

